{
  "name": "geodl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.dev -- next dev",
    "prod": "npm run build",
    "build": "npm run generate-prod && dotenv -e .env.prod -- next build",
    "start": "next start",
    "lint": "next lint",
    "deploy-dev": "dotenv -e .env.dev -- npx prisma db push --force-reset && dotenv -e .env.dev -- npm run generate-value-types && dotenv -e .env.dev -- npx prisma db seed",
    "deploy-prod": "dotenv -e .env.prod -- npx prisma db push --force-reset && dotenv -e .env.prod -- npm run generate-value-types && dotenv -e .env.prod -- npx prisma db seed",
    "generate-dev": "dotenv -e .env.dev -- npx prisma generate && dotenv -e .env.dev -- npm run generate-value-types",
    "generate-prod": "dotenv -e .env.prod -- npm run generate-value-types",
    "generate-value-types": "tsx prisma/generators/*.ts",
    "generate-geojson": "tsx generateGeoJson.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@next/env": "^14.2.3",
    "@prisma/client": "^5.12.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@react-three/drei": "^9.105.6",
    "@react-three/fiber": "^8.16.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.1",
    "csv-parse": "^5.5.5",
    "embla-carousel-react": "^8.0.2",
    "framer-motion": "^11.1.8",
    "lucide-react": "^0.373.0",
    "next": "14.2.2",
    "next-themes": "^0.3.0",
    "pigeon-maps": "^0.21.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.1.0",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.164.1",
    "three-globe": "^2.31.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.12.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cli-progress": "^3.12.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "^9.1.0",
    "geojson-places": "^1.0.8",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "react-device-detect": "^2.2.3",
    "sass": "^1.75.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
